{
  "name": "rosszurowski",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production next build && next export -o dist && yarn build:404 && yarn build:dat",
    "build:404": "mv dist/_error/index.html dist/404.html && rm -r dist/_error",
    "build:dat": "cp dat.json dist/dat.json && mkdir -p dist/.well-known && touch dist/.well-known/dat && echo $(node -p 'require(\"./dat.json\").url') > dist/.well-known/dat && echo \"TTL=0\" >> dist/.well-known/dat",
    "start": "next",
    "deploy": "echo '> No custom deploy script, use `git push` instead'",
    "lint": "eslint {pages,lib,components}/"
  },
  "dependencies": {
    "canvas-fit": "^1.5.0",
    "lazysizes": "^4.0.0-rc1",
    "marksy": "^5.0.0",
    "next": "^3.0.3",
    "prop-types": "^15.5.10",
    "randf": "^1.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "scrollmonitor": "^1.2.3",
    "styled-jsx": "^0.5.7",
    "tinytime": "^0.2.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.5.0",
    "eslint-config-watsi": "^1.0.15",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.53.1"
  }
}
