    "autoprefixer": "^10.4.7",
    "contentlayer": "^0.2.9",
    "date-fns": "^2.29.1",
    "eslint": "8.20.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "excerpt-html": "^1.2.2",
    "feed": "^4.2.2",
    "globby": "^13.1.2",
    "next-contentlayer": "^0.2.9",
    "next-plausible": "^3.6.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rehype-external-links": "^2.0.0",
    "remark-gfm": "^3.0.1",
    "remark-smartypants": "^2.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "next",
      "next/core-web-vitals",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
