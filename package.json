{
  "name": "@rosszurowski/rosszurowski.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build && next export -o dist && yarn build:cname && yarn build:404",
    "build:cname": "echo 'rosszurowski.com' > dist/CNAME",
    "build:404": "mv dist/_error/index.html dist/404.html && rm -r dist/_error",
    "start": "next",
    "deploy": "touch dist/.nojekyll && gh-pages -t -d dist -b master",
    "predeploy": "yarn build"
  },
  "dependencies": {
    "canvas-loop": "^1.0.7",
    "lazysizes": "^4.0.0-rc1",
    "next": "beta",
    "prop-types": "^15.5.10",
    "randf": "^1.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-medium-image-zoom": "^1.0.3",
    "recompose": "^0.23.4",
    "scrollmonitor": "^1.2.3",
    "styled-jsx": "^0.5.7"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-watsi": "^1.0.15",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "gh-pages": "^1.0.0"
  }
}
